	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Run Dev Server",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/server.js",
			"args": ["dev"],
			"serverReadyAction": {
				"action": "openExternally",
				"uriFormat": "http://localhost:%s"
			},
		}
	]
}